{
    "name": "cortex/auth-b2b2c2",
    "description": "Cortex Auth B2B2C2 (Business to Business, Business to Customer, Business to Business to Customer) is a complementary module for cortex/auth which is frontend layer for the powerful authentication, authorization and verification package rinvex/laravel-auth on top of Laravel.",
    "type": "cortex-module",
    "keywords": [
        "authentication",
        "authorization",
        "verification",
        "activation",
        "abilities",
        "cortex",
        "phone",
        "sms",
        "call",
        "users",
        "module",
        "security",
        "twofactor",
        "laravel",
        "rinvex",
        "rbac",
        "totp",
        "acl",
        "roles"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-auth/issues",
        "source": "https://github.com/rinvex/cortex-auth",
        "docs": "https://github.com/rinvex/cortex-auth/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-auth/contributors"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "Illuminate/auth": "~5.8.0",
        "Illuminate/console": "~5.8.0",
        "Illuminate/http": "~5.8.0",
        "Illuminate/routing": "~5.8.0",
        "Illuminate/support": "~5.8.0",
        "cortex/foundation": "^2.0.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.2.0",
        "laravel/socialite": "^3.0.0",
        "laravelcollective/html": "~5.8.0",
        "league/fractal": "^0.18.0",
        "pragmarx/google2fa": "^4.0.0",
        "proengsoft/laravel-jsvalidation": "^2.4.0",
        "propaganistas/laravel-phone": "^4.1.0",
        "rinvex/laravel-auth": "^2.0.0",
        "rinvex/laravel-composer": "^2.0.0",
        "rinvex/countries": "^6.0.0",
        "rinvex/laravel-menus": "^2.0.0",
        "rinvex/laravel-support": "^2.0.0",
        "rinvex/laravel-tenants": "^2.0.0",
        "silber/bouncer": "v1.0.0-rc.5",
        "spatie/laravel-activitylog": "^3.6.0",
        "yajra/laravel-datatables-buttons": "^4.6.0",
        "yajra/laravel-datatables-fractal": "^1.4.0",
        "yajra/laravel-datatables-html": "^4.4.0",
        "yajra/laravel-datatables-oracle": "^9.3.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.0",
        "phpunit/phpunit": "^8.1.0"
    },
    "autoload": {
        "psr-4": {
            "Cortex\\Auth\\B2B2C2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Auth\\B2B2C2\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Auth\\B2B2C2\\Providers\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
